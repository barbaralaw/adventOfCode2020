treeCount = 0;

function mapForest(forestArray) {
  for (i=0; i < forestArray.length; i++ ) {
      let toCheck = (i*3) % 31;
      if (forestArray[i].charAt(toCheck) === '#') {
        treeCount = treeCount + 1;
      }
  }
  return (`You will hit ${treeCount} trees.`)
};

